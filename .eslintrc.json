{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"

    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
    ],
    "rules": {
      "no-use-before-define": "off",
      "no-console": "off",
      "class-methods-use-this": "off",
      "no-param-reassign": "off",
       "no-unused-expressions": "off",
       "camelcase": "off"
    }
}
